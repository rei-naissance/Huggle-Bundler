DATABASE_URL=postgresql+psycopg://username:password@your-database-host:5432/your-database-name?sslmode=require
APP_ENV=development
APP_DEBUG=true
CORS_ORIGINS=*

DB_CONNECT_TIMEOUT=10

# AI
AI_PROVIDER=groq
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_api_key_here
OPENROUTER_MODEL=deepseek/deepseek-chat-v3.1:free
GROQ_API_KEY=gsk_your_groq_api_key_here
GROQ_MODEL=gemma2-9b-it

# Local Image Generation API
LOCAL_IMAGE_API_URL=https://image.huggle.tech
IMAGE_GENERATION_TIMEOUT=60
USE_MOCK_IMAGES=false

# CloudFlare R2 Image Storage
R2_BUCKET_NAME=your-r2-bucket-name
R2_ENDPOINT_URL=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_REGION=auto
R2_PUBLIC_DOMAIN=https://huggle.tech
